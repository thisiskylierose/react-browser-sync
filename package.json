  },
  "homepage": "https://github.com/thisiskylierose/react-browser-sync#readme",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf www/assets && rm -rf www/data",
    "slate": "rm -rf node_modules && npm install",
    "clean:win": "rmdir /s /q www/assets && rmdir /s /q www/data",
    "slate:win": "rmdir /s /q node_modules && npm install",
    "start": "npm-run-all build -p watch:build start:server",
    "start:server": "node server.js",
    "watch:build": "webpack --watch --config src/webpack.config.js",
    "build": "webpack --config src/webpack.config.js",
    "lint": "eslint --ext .js src/",
    "test": "test"
  },
