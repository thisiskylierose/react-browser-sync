    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "browser-sync": "2.26.3",
    "connect-browser-sync": "2.1.0",
    "css-loader": "2.0.0",
    "ejs": "2.6.1",
    "eslint": "5.10.0",
